{
  "name": "react-typewriter-hook",
  "version": "2.0.1",
  "description": "⌨️ Use react hooks for typing effect easily",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "example:dev": "npm --prefix example run dev",
    "example:build": "npm --prefix example run build",
    "example:deploy": "npm --prefix example run deploy",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haowen737/react-typewriter-hook.git"
  },
  "keywords": [
    "react-hooks"
  ],
  "author": "Haowen <haowen737@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haowen737/react-typewriter-hook/issues"
  },
  "engines": {
    "node": ">=20"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@testing-library/react": "^16.0.0",
    "jsdom": "^28.0.0",
    "react": "^18.3.1",
    "react-dom": "^19.2.4",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.17"
  },
  "dependencies": {}
}
